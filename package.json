{
  "name": "type-api",
  "version": "1.0.0",
  "main": "index.js",
  "author": "rsbear",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "migration:create": "run migration:create -n all",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n all",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "install": "",
    "build": "tsc",
    "build-ts": "tsc",
    "postinstall": "yarn build-ts",
    "start": "yarn serve",
    "serve": "NODE_ENV=production node ./public/api/index.js",
    "cleanbuild": "rm -rf public",
    "deploy": "yarn cleanbuild && yarn build && git add . && git commit -m 'deploy' && git push heroku master",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "@jorgeferrero/stream-to-buffer": "^1.0.0",
    "@sendgrid/mail": "^6.5.1",
    "apollo-server-express": "^2.9.7",
    "aws-sdk": "^2.568.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "date-fns": "^2.7.0",
    "date-fns-timezone": "^0.1.4",
    "dayjs": "^1.8.17",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-upload": "^8.1.0",
    "imagemin": "^7.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.2",
    "nodemailer-mailgun-transport": "^2.0.0",
    "now-storage-v2": "^2.0.2",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "rword": "^3.1.1",
    "sharp": "^0.23.4",
    "shortid": "^2.2.15",
    "type-graphql": "^0.18.0-beta.9",
    "typeorm": "^0.2.20",
    "uniqid": "^5.1.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-mozjpeg": "^8.0.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.6",
    "@types/node-fetch": "^2.5.4",
    "@types/nodemailer": "^6.4.0",
    "@types/nodemailer-mailgun-transport": "^1.4.1",
    "@types/request": "^2.48.4",
    "@types/sharp": "^0.23.0",
    "@types/shortid": "^0.0.29",
    "@types/uniqid": "^4.1.3",
    "nodemon": "^1.19.4",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  }
}
